{
  "name": "@sooho/cli",
  "description": "CLI tool to interact with Sooho",
  "version": "0.3.0",
  "author": "Jisu Park <jisupark@sooho.io>",
  "bin": {
    "sooho": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soohoio/sooho"
  },
  "bugs": "https://github.com/soohoio/sooho/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2.1.4",
    "@oclif/plugin-update": "^1.3.9",
    "@sooho/parser": "^0.3.0",
    "ora": "^3.0.0",
    "powerwalker": "^0.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "aws-sdk": "^2.335.0",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/soohoio/sooho",
  "keywords": [
    "sooho",
    "security",
    "assessments",
    "contract",
    "solidity",
    "vulnerability"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sooho",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "update": {
      "s3": {
        "bucket": "sooho-cli-assets"
      },
      "node": {
        "version": "10.12.0"
      }
    },
    "macos": {
      "sign": "Developer ID Installer: Jisu Park",
      "identifier": "com.sooho.cli"
    }
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
